{
  "name": "webdriver-sync",
  "version": "0.4.3",
  "description": "Porting the Java WebDriver to node for a completely synchronous selenium experience in node.js!",
  "main": "src/webdriver-sync.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jshint --config .jshintrc src/**/*.js && jshint --config .test-jshintrc test/**/*.js && node_modules/.bin/mocha -t 8000 test/*.js test/classes/*.js",
    "preinstall": "node bin/install.js pre"
  },
  "dependencies": [
    "java"
  ],
  "devDependencies": [
    "mocha",
    "proxyquire",
    "sinon"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/webdriver-sync.git"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "testing",
    "sync",
    "synchronous",
    "api",
    "chrome",
    "safari",
    "internet explorer",
    "explorer",
    "internet",
    "ie",
    "driver",
    "node",
    "javascript"
  ],
  "author": "Joseph Spencer",
  "license": "Apache2.0",
  "bugs": {
    "url": "https://github.com/jsdevel/webdriver-sync/issues"
  }
}
