{
  "name": "webdriver-sync",
  "version": "0.18.0",
  "description": "Selenium testing without nested callbacks or promises!",
  "main": "src/webdriver-sync.js",
  "scripts": {
    "lint-src": "node_modules/.bin/jshint --config .jshintrc src/**/*.js",
    "lint-test": "node_modules/.bin/jshint --config .test-jshintrc test/**/*.js",
    "mocha": "node_modules/.bin/mocha -r should --no-timeouts test/*.js test/classes/*.js test/enums/*.js test/interfaces/*.js",
    "test": "npm run lint-src && npm run lint-test && npm run mocha"
  },
  "dependencies": {
    "async": "~0.2.9",
    "java": "0.8.0",
    "selenium-binaries": "0.10.x"
  },
  "devDependencies": {
    "jshint": "2.3.0",
    "mocha": "^1.21.3",
    "proxyquire": "^1.0.1",
    "should": "*",
    "sinon": "^1.10.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsdevel/webdriver-sync.git"
  },
  "keywords": [
    "webdriver",
    "selenium",
    "testing",
    "sync",
    "synchronous",
    "api",
    "chrome",
    "safari",
    "internet explorer",
    "explorer",
    "internet",
    "ie",
    "driver",
    "node",
    "javascript"
  ],
  "author": "Joseph Spencer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsdevel/webdriver-sync/issues"
  }
}
